#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir//home/karan/.nvm/versions/node/v21.7.1/bin/node" ]; then
  exec "$basedir//home/karan/.nvm/versions/node/v21.7.1/bin/node"  "$basedir/../karan/index.js" "$@"
else 
  exec /home/karan/.nvm/versions/node/v21.7.1/bin/node  "$basedir/../karan/index.js" "$@"
fi
